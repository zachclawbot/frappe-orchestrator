# Database Configuration
DB_ROOT_PASSWORD=RootPassword123
DB_USER=frappe
DB_PASSWORD=FrappePassword123
DB_NAME=frappe

# Frappe Configuration
FRAPPE_SECRET_KEY=your-secret-key-change-in-production
FRAPPE_ENVIRONMENT=development
ENABLE_SCHEDULER=1

# GCP Configuration (for production deployment)
GCP_PROJECT_ID=your-gcp-project
GCP_REGION=us-central1
GCP_CLOUDSQL_INSTANCE=frappe-orchestrator-prod

# Email Configuration
MAIL_SERVER=smtp.gmail.com
MAIL_PORT=587
MAIL_USE_TLS=1
MAIL_USERNAME=your-email@gmail.com
MAIL_PASSWORD=your-app-password

# Site Configuration
SITE_NAME=frappe-orchestrator.local
SITE_URL=http://frappe-orchestrator.local

# Storage Configuration
FILE_STORAGE=local
# For GCP Cloud Storage:
# FILE_STORAGE=gcs
# GCS_BUCKET=frappe-orchestrator-files
